#include <iostream>
using namespace std;
int main(){
    int A[3][3],B[3][3];
    for(int i =0;i<3;i++){
        for(int j =0;j<3;j++){
            scanf("%d", &A[i][j]);
        }

    }
    for(int i=0;i<3;i++){
        for(int j =0;j<3;j++){
              printf("%d\t", A[i][j]);
  }
        }
        printf("\n");
}

 for(int i=0;i<3;i++){
        for(int j =0;j<3;j++){
        
            B[j][i] = A[i][j];
         
        
        }
 }
 for(int i=0;i<3;i++)
 {

    for(int j =0;j<3;j++){
      printf("%d\t", B[i][j]);
    }
    
    printf("\n");
 }

    
 
 return 0 ;
}